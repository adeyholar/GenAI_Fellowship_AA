{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }, // <--- THIS COMMA IS OK IF THERE'S ANOTHER CONFIGURATION BELOW
        {
            "name": "Streamlit: Run Current File",
            "type": "python",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "${file}"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "justMyCode": true
        }// <--- THIS COMMA IS AN ERROR IF IT'S THE LAST ITEM IN THE ARRAY
    ]
}